            android:value="AIzaSyDdxLAWjk_BUBqFCwEBTVzwYqo8b3FsV88" />

        <activity android:name=".ui.Continuar"
            android:screenOrientation="portrait"  />
        <activity android:name=".ui.VistaTransecto"
            android:screenOrientation="portrait" />

        <service
